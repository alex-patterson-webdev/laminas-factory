{
  "name" : "alex-patterson-webdev/laminas-factory",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "laminas/laminas-servicemanager": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4"
  },
  "autoload": {
    "psr-4": {
      "Arp\\LaminasFactory\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\LaminasFactory\\" : "test/phpunit"
    }
  },
  "scripts": {
    "unit": "php vendor/bin/phpunit --testsuite=unit --coverage-clover=coverage.xml"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
