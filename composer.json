{
  "name" : "alex-patterson-webdev/laminas-factory",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Alex Patterson",
      "email": "alex.patterson.webdev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4 || >=8.0",
    "laminas/laminas-servicemanager": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Arp\\LaminasFactory\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\LaminasFactory\\" : "test/phpunit/"
    }
  },
  "scripts": {
    "unit": "php vendor/bin/phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
